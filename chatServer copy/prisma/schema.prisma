datasource db {
  provider = "mysql"
  url = env("ISHAON_CHAT_DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id Int @id @default(autoincrement())
  username String @unique
  password String
  created_at DateTime @default(now())
  updated_at DateTime @default(now())
}